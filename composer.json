{
    "name": "tarohida/docker-php8-nginx-postgres-composer",
    "description": "Docker Compose configuration to run PHP8 with Nginx, PHP-FPM, PostgreSQL 10.1 and Composer.",
    "type": "metapackage",
    "license": "MIT",
    "authors": [
        {
            "name": "Taro Hida",
            "email": "sk8trou@gmail.com"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "byjg/migration-cli": "4.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
